<h1>Add New Album</h1>

<%= form_for Vinyl.new do |f| %>
	<%= f.label :artist %>
	<%= f.text_field :artist %><br />

	<%= f.label :album %>
	<%= f.text_field :album %><br />

	<%= f.label :genre %>
	<%= f.text_field :genre %><br />

	<%= f.label :year %>
	<%= f.text_field :year %><br />

	<%= f.submit "Submit" %>
<% end %>